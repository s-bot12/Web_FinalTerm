                                        <a href="/rank/get">
                                        <div class="product__item__pic__sm set-bg" data-setbg="/img/game/game0.png">
                                        </div>
                                        </a>
                                        <div class="product__item__text">
                                            <h5><a href="/rank/get">Tetris</a></h5>
                                            <table class="rank_table">
                                                <tr><th>Ranking</th>
                                                    <% for (var i = 0; i < rank.length; i++) { %>
                                                        <% if (i >= 3) continue; %>
                                                        <% if (i == 0) { %>                                        
                                                            <th style="color:rgb(221, 118, 118)">&nbsp;&nbsp;<%= i+1 %></th>
                                                            <td style="color:rgb(221, 118, 118)"><%= rank[i].name %></td>
                                                            <td style="color:rgb(221, 118, 118)"><%= rank[i].point %></td>
                                                        <% continue; } %>
                                                        <tr><th></th>
                                                            <th>&nbsp;&nbsp;<%= i+1 %></th>
                                                            <td><%= rank[i].name %></td>
                                                            <td><%= rank[i].point %></td>                                                       
                                                        </tr>
                                                    <% } %>

                                                </tr>
                                            </table>
                                        </div>
                                    </div>
                                </div>
