<!DOCTYPE html>
<html lang="zxx">

    <%- include('layouts/header'); -%>


<body>

    <%- include('layouts/bar'); -%>

        <!-- Page Preloder -->
        <div id="preloder">
            <div class="loader"></div>
        </div>
    
        <!-- Breadcrumb Begin -->
        <div class="breadcrumb-option">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="breadcrumb__links">
                            <a href="/"><i class="fa fa-home"></i> Home </a>
