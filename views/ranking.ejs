                                        <div class="product__item__pic__sm set-bg" data-setbg="/img/game/game5.PNG">
                                        </div>
                                        <div class="product__item__text">
                                            <h5><a href="#">HexGL</a></h5>
                                            <table class="rank_table">
                                                <tr><th>Ranking</th>
                                                    <th style="color:rgb(221, 118, 118)">&nbsp;&nbsp;1</th>
                                                    <td style="color:rgb(221, 118, 118)">test1</td>
                                                    <td style="color:rgb(221, 118, 118)">37</td>
                                                </tr>
                                                <tr><th></th>
                                                    <th>&nbsp;&nbsp;2</th>
                                                    <td>mineman</td>
                                                    <td>25</td>
